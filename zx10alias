#need to fix if /usr/bin/bash etc
if [ "bash" == "$0" -o "-bash" == "$0" ]
then
 echo Need to fix varnames
 echo BASH_SOURCE =  $BASH_SOURCE
 #Fix if readlink unexist in os 
 D=$(cd $(dirname $BASH_SOURCE)&&pwd) 
 #F=$(readlink -e $BASH_SOURCE)
 #D=$(dirname $F)
 A=$(basename $BASH_SOURCE)
 echo D=$D A=$A

 for i in {1..9}0 {11..99}0 {1..10}000
 do 
    alias $i="zxset $i"
 done
 echo $0 aliasing $A
 #including PATH and . alias fix
  . $D/zxalias
 
#Need to fix if PATH already set
else
 echo ZX Not ok
 echo Sets ALIAS not symlinks
 echo $0
 echo USAGE
 echo . $0
fi


